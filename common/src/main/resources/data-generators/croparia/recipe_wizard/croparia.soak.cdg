@block=croparia:elemental_stone;
@path=croparia/soak_{opposite_block_namespace}_{opposite_block_path}.json;
@extensions=croparia:soak;
{
  "type": "croparia:soak",
  "element": "{soak_element}",
  "input": {
    "id": "{neighbor}",
    "components": {neighbor_properties}
  },
  "output": {
    "id": "{below}",
    "components": {below_properties}
  },
  "probability": 1.0
}
